{
    "name": "Lexia",
    "version": "1.1.0",
    "description": "Dislexic syntax highlighter",
    "manifest_version": 3,
    "author": "Ben Feldberg",
    "action": {
        "default_popup": "options.html",
        "default_title": "Lexia"
